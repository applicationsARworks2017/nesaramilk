<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_my_subscription"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <android.support.v7.widget.CardView
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Product :"
                    android:textColor="@color/black"
                    android:id="@+id/p_head"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/p_head"
                    android:text="Organic Milk,2.5 lt"
                    android:id="@+id/p_type"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/p_head" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/p_head"
                    android:layout_marginTop="5dp"
                    android:text="Delivery type : "
                    android:textColor="@color/black"
                    android:id="@+id/d_head" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/p_type"
                    android:layout_toEndOf="@+id/d_head"
                    android:layout_marginTop="5dp"
                    android:id="@+id/d_type"
                    android:textStyle="bold"
                    android:text="Organic Milk,2.5 lt"
                    android:layout_marginLeft="10dp"
                   />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/d_head"
                    android:layout_marginTop="5dp"
                    android:text="Start Date : "
                    android:textColor="@color/black"
                    android:id="@+id/s_date"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/d_type"
                    android:layout_toEndOf="@+id/s_date"
                    android:layout_marginTop="5dp"
                    android:text="Dec 30,2016"
                    android:textStyle="bold"
                    android:id="@+id/s_date_value"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/s_date" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/s_date"
                    android:layout_marginTop="5dp"
                    android:text="End Date : "
                    android:textColor="@color/black"
                    android:id="@+id/e_date" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/s_date_value"
                    android:layout_toEndOf="@+id/e_date"
                    android:layout_marginTop="5dp"
                    android:text="Dec 30,2016"
                    android:textStyle="bold"
                    android:id="@+id/e_date_value"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/e_date" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/e_date_value"
                    android:layout_marginTop="5dp"
                    android:text="Total : "
                    android:textColor="@color/black"
                    android:id="@+id/e_total" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/e_date"
                    android:layout_toEndOf="@+id/e_date"
                    android:layout_marginTop="5dp"
                    android:text=""
                    android:textStyle="bold"
                    android:id="@+id/e_total_value"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/e_date" />



             <!--   <include layout="@layout/edit_play_stop"
                    android:id="@+id/layout_option"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_alignParentRight="true"></include>-->
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:id="@+id/subline"
                    android:background="@color/gray"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/e_total_value"></LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/subline"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/iv_edit"
                        android:background="@mipmap/ic_mode_edit_black_24dp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/iv_pause"
                        android:layout_marginLeft="40dp"
                        android:background="@mipmap/ic_pause_black_24dp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/iv_resume"
                        android:layout_marginLeft="40dp"
                        android:background="@mipmap/ic_play_arrow_black_24dp"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/iv_stop"
                        android:layout_marginLeft="40dp"
                        android:background="@mipmap/ic_stop_black_24dp"/>
                </LinearLayout>

            </RelativeLayout>

           <!-- <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/menub"
                android:id="@+id/menuB"
                android:layout_marginTop="15dp"
                android:layout_gravity="right"
                android:layout_marginRight="10dp" />

            <include layout="@layout/edit_play_stop"
                android:id="@+id/layout_option"
                android:visibility="gone"
                android:layout_below="@+id/menuB"
                android:layout_marginTop="35dp"
                android:layout_marginRight="35dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"></include>-->
        </android.support.v7.widget.CardView>


</RelativeLayout>
